    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@typescript-plus/stream-capture-20211002": "^0.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jasmine": "^3.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.5",
    "typescript": "^4.4.3"
